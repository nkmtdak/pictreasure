<h1>Challenges#new</h1>
<p>Find me in app/views/challenges/new.html.erb</p>
<h1>New Challenge</h1>

<%= form_with(model: @challenge, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>

  <%= form.submit %>
<% end %>