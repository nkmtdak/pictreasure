<h1>Upload Photo for <%= @challenge.title %></h1>

<%= form_with(model: [@challenge, @photo], local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>

  <%= form.submit 'Upload Photo' %>
<% end %>